<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="in.co.rays.dto" >

	<class name="SubjectDTO" table="ST_SUBJECT">
		<id name="id" type="java.lang.Long">
			<column name="ID"></column>
			<generator class="increment"></generator>
		</id>
		
		<property name="subjectName" type="java.lang.String" length="50">
			<column name="SUBJECT_NAME" />
		</property>
		
		<property name="courseName" type= "java.lang.String" length="50">
			<column name="COURSE_NAME" />
		</property>
		
		<property name="subjectId" type= "java.lang.Long" length="50">
			<column name="SUBJECT_ID" />
		</property>
		
		<property name="courseId" type= "java.lang.Long" length="50">
			<column name="COURSE_ID" />
		</property>
		
		<property name="description" type= "java.lang.String" length="50">
			<column name="DESCRIPTION" />
		</property>
		

		<property name="createdBy" type="java.lang.String" length="50">
			<column name="CREATED_BY" />
		</property>
		<property name="modifiedBy" type="java.lang.String" length="50">
			<column name="MODIFIED_BY" />
		</property>
		<property name="createdDatetime" type="java.sql.Timestamp">
			<column name="CREATED_DATETIME" />
		</property>
		<property name="modifiedDatetime" type="java.sql.Timestamp">
			<column name="MODIFIED_DATETIME" />
		</property>
	</class>
</hibernate-mapping>
